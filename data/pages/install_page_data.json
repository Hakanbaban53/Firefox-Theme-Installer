{
  "data_paths": {
    "ASSETS_PATH": "assets",
    "CACHE_PATH_unix": "~/.cache/Firefox Theme Installer/",
    "CACHE_PATH_win": "C:\\Users\\%USERNAME%\\AppData\\Local\\FirefoxThemeInstaller\\",
    "NAVIGATION_BUTTON_DATA_PATH": "data/components/navigation_button_data.json",
    "OS_PROPERTIES_PATH": "data/OS data/os_properties.json",
    "INPUTS_DATA_PATH": "data/components/inputs_data.json"
  },
  "icons": {
    "attention_icon": "icons/attention.png",
    "header_title_bg": "backgrounds/header_title_background.png",
    "line_top_img": "backgrounds/line_top.png",
    "preview_icon": "icons/preview_icon.png",
    "theme_detected_icon": "icons/theme-selected.png"
  },
  "header_data": {
    "text": "Check the Install Config"
  },
  "create_inputs": {
    "inputs_frame": {
      "width": 440,
      "height": 54,
      "corner_radius": 12,
      "fg_color": "#2B2631",
      "grid_data": {
        "row": 3,
        "column": 0,
        "columnspan": 2,
        "padx": 40,
        "pady": [10, 20],
        "sticky": "NSEW"
      }
    }
  },
  "create_preview_theme": {
    "preview_frame": {
      "width": 440,
      "height": 60,
      "corner_radius": 12,
      "fg_color": "white",
      "grid_data": {
        "row": 0,
        "column": 1,
        "padx": 40,
        "pady": [20, 30],
        "sticky": ""
      }
    },
    "preview_label": {
      "text": "Preview Theme in Firefox",
      "text_color": "black",
      "font": "(\"Arial\", 18, \"bold\")",
      "pack_data": {
        "padx": 10,
        "pady": 10,
        "side": "top"
      }
    },
    "preview_button": {
      "text": "Preview Theme",
      "fg_color": "#D9D9D9",
      "hover_color": "#EEEEEE",
      "text_color": "#000000",
      "height": 42,
      "font": "(\"Arial\", 16, \"bold\")",
      "corner_radius": 12,
      "pack_data": {
        "padx": 10,
        "pady": 10
      }
    }
  },
  "create_invalid_entry_frame": {
    "invalid_entry_frame": {
      "width": 440,
      "height": 54,
      "corner_radius": 12,
      "fg_color": "white",
      "grid_data": {
        "row": 5,
        "column": 0,
        "padx": [10, 4],
        "pady": 10
      }
    },
    "invalid_entries_text": {
      "text": "",
      "text_color": "#f04141",
      "font": "(\"Arial\", 16, \"bold\")",
      "compound": "left",
      "pack_data": {
        "padx": 10,
        "pady": 10
      }
    }
  },
  "update_button_and_frame": {
    "invalid_entries_text": {
      "text": " entries is invalid"
    }
  }
}
